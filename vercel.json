    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/((?!_expo/|assets/|favicon.ico|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]